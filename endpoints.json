        "users": [
            {
                "uid": "initial_user",
                "email": "test@sdsf.com",
                "bio": "dsf sfstfsd sdfsdf ",
                "skill_level": "prof",
                "location": {
                    "_latitude": 61.355591,
                    "_longitude": 15.882829
                },
                "username": "test2",
                "preferences": [
                    "uphill",
                    "dog friendly"
                ],
                "avatar_id": 5
            }
        ]
      }
    },
